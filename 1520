#include <iostream>
int roff[4] = { -1,1,0,0 };
int coff[4] = { 0,0,1,-1 };
using namespace std;

int arr[501][501];
int dp[501][501];
int n, m;
int go(int x, int y)
{
	if (x == n && y == m)
		return 1;
	if (dp[x][y] != -1)
		return dp[x][y];
	dp[x][y] = 0;
	for (int d = 0; d < 4; d++)
	{
		int nr = x + roff[d];
		int nc = y + coff[d];
		if (nr <1 || nc<1 || nr>n || nc>m)
			continue;
		if (arr[nr][nc] >= arr[x][y])
			continue;
		dp[x][y] += go(nr, nc);
	}
	return dp[x][y];
}
int main()
{
	cin >> n >> m;

	for (int i = 1; i <= n; i++)
		for (int j = 1; j <= m; j++)
		{
			cin >> arr[i][j];
			dp[i][j] = -1;
		}
	cout << go(1, 1) << endl;
	return 0;
}
