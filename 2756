#include <iostream>
#include <math.h>
using namespace std;
double arr1[3][2];
double arr2[3][2];
int T;
int cal(double dist)
{
	if (dist <= 3)
		return 100;
	else if (dist <= 6)
		return 80;
	else if (dist <= 9)
		return 60;
	else if (dist <= 12)
		return 40;
	else if (dist <= 15)
		return 20;
	else return 0;
}
int main()
{
	cin >> T;
	for (int test = 0; test < T; test++)
	{
		for (int i = 0; i < 3; i++)
			cin >> arr1[i][0] >> arr1[i][1];
		for (int i = 0; i < 3; i++)
			cin >> arr2[i][0] >> arr2[i][1];
		int score1 = 0, score2 = 0;
		for (int i = 0; i < 3; i++)
		{
			double temp1 = sqrt(pow(arr1[i][0], 2) + pow(arr1[i][1], 2));
			double temp2 = sqrt(pow(arr2[i][0], 2) + pow(arr2[i][1], 2));
			score1 += cal(temp1);
//			cout << score1 << endl;
			score2 += cal(temp2);
//			cout << score2 << endl;
		}
		cout << "SCORE: " << score1 << " to " << score2 << ",";
		if (score1 > score2)
			cout << " PLAYER 1 WINS." << endl;
		else if (score1 < score2)
			cout << " PLAYER 2 WINS." << endl;
		else
			cout << " TIE." << endl;
	}
	return 0;
}
