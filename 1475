#include <iostream>
#include <string>

using namespace std;
int arr[10];
int main()
{
	string str;
	getline(cin, str);
	for (int i = 0; i < str.length(); i++)
	{
		int temp = str.at(i) - '0';
		arr[temp]++;
	}
	arr[6] += arr[9];
	if (arr[6] % 2 == 0)
		arr[6] /= 2;
	else
		arr[6] /= 2, arr[6]++;
	int max = 0;
	for (int i = 0; i < 9; i++)
		if (max < arr[i])
			max = arr[i];
	cout << max << endl;
	return 0;
}
