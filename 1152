#include <iostream>
#include <string>
using namespace std;

int main()
{
	string ch;
	int cnt = 0;
	getline(cin,ch);
	for (int i = 0; i < ch.length(); i++)
	{
		if (ch.at(i) != ' ' &&i !=0 &&  ch.at(i-1)==' ')
			cnt++;
	}
	if (ch.at(0) == ' ')
		cout << cnt << endl;
	else
		cout << cnt+1 << endl;
	return 0;
}
