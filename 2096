#include <iostream>
#include <algorithm>
using namespace std;
int n;
int arr[100000][3];
int dp[2][3];
int main()
{
	cin >> n;
	for (int i = 0; i <n; i++)
		cin >> arr[i][0] >> arr[i][1] >> arr[i][2];
	dp[0][0] = arr[0][0];
	dp[0][1] = arr[0][1];
	dp[0][2] = arr[0][2];
	for (int i = 1; i < n; i++)
	{
		dp[1][0] = max(dp[0][1], dp[0][0]) + arr[i][0];
		dp[1][1] = max(dp[0][0], max(dp[0][1], dp[0][2])) + arr[i][1];
		dp[1][2] = max(dp[0][1], dp[0][2]) + arr[i][2];
		dp[0][0] = dp[1][0];
		dp[0][1] = dp[1][1];
		dp[0][2] = dp[1][2];
	}
	cout << max(dp[0][0], max(dp[0][1], dp[0][2])) << ' ';
	dp[0][0] = arr[0][0];
	dp[0][1] = arr[0][1];
	dp[0][2] = arr[0][2];
	for (int i = 1; i < n; i++)
	{
		dp[1][0] = min(dp[0][1], dp[0][0]) + arr[i][0];
		dp[1][1] = min(dp[0][0], min(dp[0][1], dp[0][2])) + arr[i][1];
		dp[1][2] = min(dp[0][1], dp[0][2]) + arr[i][2];
		dp[0][0] = dp[1][0];
		dp[0][1] = dp[1][1];
		dp[0][2] = dp[1][2];
	}
	 cout<< min(dp[0][0], min(dp[0][1], dp[0][2])) << endl;
	return 0;
}
