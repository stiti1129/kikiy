#include <iostream>
#include <string.h>
#include <algorithm>
using namespace std;
int result = 64;
int n, m;
char temp[50][50];
int go(int x, int y)
{
	int num1 = 0;
	int num2 = 0;
	for (int i = 0; i < 8; i++)
	{
		for (int j = 0; j < 8; j++)
		{
			if ((i + j )% 2 == 0 && temp[x + i][y + j] == 'B')
				num1++;
			if ((i + j) % 2 == 1 && temp[x + i][y + j] == 'W')
				num1++;
		}
	}
	for (int i = 0; i < 8; i++)
	{
		for (int j = 0; j < 8; j++)
		{
			if ((i + j) % 2 == 0 && temp[x + i][y + j] == 'W')
				num2++;
			if ((i + j) % 2 == 1 && temp[x + i][y + j] == 'B')
				num2++;
		}
	}
	return min(num1, num2);
}

int main()
{
	cin >> n >> m;
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
			cin >> temp[i][j];
	for (int i = 0; i + 8 <= n; i++)
		for (int j = 0; j + 8 <= m; j++)
			result = min(result,go(i, j));
	cout << result << endl;
	return 0;
}
