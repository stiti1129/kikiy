#include <iostream>
#include <string>

using namespace std;

int main()
{
	string str; 
	cin >> str;
	int cnt = 0;

	for (int i = 0; i < str.length(); i++)
	{
		if (str.at(i) == 'c')
		{
			if (i +1< str.length() &&str.at(i + 1) == '=' || str.at(i + 1) == '-')
				i++, cnt++;
			else
				cnt++;
		}
		else if (str.at(i) == 'd')
		{
			if (i + 2< str.length() && str.at(i + 1) == 'z' && str.at(i + 2) == '=')
				i += 2, cnt++;
			else if (i + 1< str.length() && str.at(i + 1) == '-')
				i++, cnt++;
			else
				cnt++;
		}
		else if (str.at(i) == 'l')
		{
			if (i + 1< str.length() && str.at(i + 1) == 'j')
				i++, cnt++;
			else
				cnt++;
		}
		else if (str.at(i) == 'n')
		{
			if (i + 1< str.length() && str.at(i + 1) == 'j')
				i++, cnt++;
			else
				cnt++;
		}
		else if (str.at(i) == 's')
		{
			if (i + 1< str.length() && str.at(i + 1) == '=')
				i++, cnt++;
			else
				cnt++;
		}
		else if (str.at(i) == 'z')
		{
			if (i + 1< str.length() && str.at(i + 1) == '=')
				i++, cnt++;
			else
				cnt++;
		}
		else
			cnt++;
	}
	cout << cnt << endl;
	return 0;
}
