#include <iostream>
#include <algorithm>
#include <math.h>
using namespace std;

int dp[257][257];
int winner[257][257];
int arr[257];
int n;
int go(int i, int j);
int main()
{
	cin >> n;
	for (int i = 1; i <= n; i++)
		cin >> arr[i];
	for (int i = 1; i <= n; i++)
	{
		winner[i][i] = arr[i];
		for (int j = i + 1; j <= n; j++)
			winner[i][j] = min(winner[i][j - 1], arr[j]);
	}
	cout << go(1, n) << endl;
	return 0;
}

int go(int i, int j)
{
	if (dp[i][j]) return dp[i][j];
	if (i == j) return 0;
	int &ans = dp[i][j];
	ans = 1000000;
	for (int k = i; k < j; k++)
	{
		if (ans > (go(i, k) + go(k + 1, j) + abs(winner[i][k] - winner[k + 1][j]))||ans ==1000000)
			ans = (go(i, k) + go(k + 1, j) + abs(winner[i][k] - winner[k + 1][j]));
	}
	return ans;
}
