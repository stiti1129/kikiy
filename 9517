#include <iostream>
using namespace std;
int ktime = 210;
int arr[101];
char arr2[101];
int n;
int k;
int main()
{
	cin >> k;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		cin >> arr[i] >> arr2[i];
	}
	int q = 1;
	while(1)
	{
		ktime -= arr[q];
		if (ktime <= 0) break;
		if (arr2[q] == 'T')
		{
			if (k == 8)
				k = 1;
			else
				k++;
		}
		q++;
	} 
	cout << k << endl;
	return 0;
}
