#include <iostream>
#include<string.h>

using namespace std;

int dp[18][18];
int n, m, k;
int main()
{
	int n, m, k;
	cin >> n >> m >> k;
	int x, y;
	if (k%m == 0)
		x = (k / m);
	else
		x = (k / m) + 1;
	if (k%m == 0)
		y = m;
	else
		y = k%m;
	if (k == 0)
	{
		dp[1][1] = 1;
		for (int i = 1; i <= n; i++)
			for (int j =1; j <= m; j++)
			{
				dp[i][j + 1] += dp[i][j];
				dp[i + 1][j] += dp[i][j];
			}
		cout << dp[n][m] << endl;
	}
//	cout << x << ' ' << y << endl;
	else
	{
		dp[1][1] = 1;
		for (int i = 1; i <= x; i++)
			for (int j = 1; j <= y; j++)
			{
				dp[i][j + 1] += dp[i][j];
				dp[i + 1][j] += dp[i][j];
			}
		int result = dp[x][y];
		memset(dp, 0, sizeof(dp));
		dp[x][y] = 1;
		for (int i = x; i <= n; i++)
			for (int j = y; j <= m; j++)
			{
				dp[i][j + 1] += dp[i][j];
				dp[i + 1][j] += dp[i][j];
			}
		//	cout << result << ' ' << dp[n][m] << endl;
		cout << result * dp[n][m] << endl;
	}
	return 0;
}
