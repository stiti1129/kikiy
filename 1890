#include <iostream>

using namespace std;
int arr[102][102];
int n;
long long dp[102][102];
long long go(int x, int y)
{
	if (x == n && y == n)
	{
		return 1;
	}
	if (!arr[x][y])
		return 0;
	if (dp[x][y])
		return dp[x][y];
	int nr = x + arr[x][y];
	if ( nr <= n)
		dp[x][y] +=go(nr, y);
	int nc = y + arr[x][y];
	if (nc <= n)
		dp[x][y] +=go(x, nc);
	return dp[x][y];
}
int main()
{
	cin >> n;
	for (int i = 1; i <= n; i++)
		for (int j = 1; j <= n; j++)
			cin >> arr[i][j];
	cout <<go(1,1) << endl;
	return 0;
}
