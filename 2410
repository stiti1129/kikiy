#include <iostream>

using namespace std;
long long arr[20]; // 0 to 19
long long dp[1000001];
int n;
int main()
{
	cin >> n;
	for (int i = 0; i < 20; i++)
		arr[i] = 1 << i;
	dp[0] = 1;
	for (int i = 0; i < 20; i++)
		for (int j = 1; j <= n; j++)
			if (j - arr[i] >= 0)
			{
				dp[j] += dp[j - arr[i]];
				dp[j] %= 1000000000;
			}
	cout << dp[n] << endl;
	return 0;
}
