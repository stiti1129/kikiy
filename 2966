#include <iostream>

using namespace std;

int n;
int ans[3];
char arr[101];
void s()
{
	for (int i = 1; i <= n; i++)
	{
		if (i % 3 == 1)
			if (arr[i] == 'A')
				ans[0]++;
		if (i % 3 == 2)	
			if (arr[i] == 'B')
				ans[0]++;
		if (i % 3 == 0)
			if (arr[i] == 'C')
				ans[0]++;
	}
}
void c()
{
	for (int i = 1; i <= n; i++)
	{
		if (i % 4 == 1)
			if (arr[i] == 'B')
				ans[1]++;
		if (i % 4 == 2)
			if (arr[i] == 'A')
				ans[1]++;
		if (i % 4 == 3)
			if (arr[i] == 'B')
				ans[1]++;
		if (i % 4 == 0)
			if (arr[i] == 'C')
				ans[1]++;
	}
}
void h()
{
	for (int i = 1; i <= n; i++)
	{
		if (i % 6 == 1)
			if (arr[i] == 'C')
				ans[2]++;
		if (i % 6 == 2)
			if (arr[i] == 'C')
				ans[2]++;
		if (i % 6 == 3)
			if (arr[i] == 'A')
				ans[2]++;
		if (i % 6 == 4)
			if (arr[i] == 'A')
				ans[2]++;
		if (i % 6 == 5)
			if (arr[i] == 'B')
				ans[2]++;
		if (i % 6 == 0)
			if (arr[i] == 'B')
				ans[2]++;
	}
}
int main()
{
	cin >> n;
	for (int i = 1; i <= n; i++)
		cin >> arr[i];
	s(); c(); h();
	int max = 0;
	for (int i = 0; i <= 2; i++)
		if (ans[i] > max)
			max = ans[i];
	cout << max << endl;
	for(int i=0;i<=2;i++)
		if (ans[i] == max)
		{
			if (i == 0)
				cout << "Adrian" << endl;
			else if (i == 1)
				cout << "Bruno" << endl;
			else
				cout << "Goran" << endl;
		}
	return 0;
}
