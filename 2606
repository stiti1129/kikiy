#include <iostream>
using namespace std;
int arr[101][101];
int n, m;

int main()
{
	cin >> n >> m;
	int a, b;
	for (int i = 1; i <= m; i++)
	{
		cin >> a >> b;
		arr[a][b] = 1;
		arr[b][a] = 1;
	}
	for (int k = 1; k <= n; k++)
	{
		for (int i = 1; i <= n; i++)
		{
			for (int j = 1; j <= n; j++)
			{
				if (i == j || i == k || j == k)
					continue;
				if (arr[i][k] && arr[k][j])
				{
					arr[i][j] = 1;
					arr[j][i] = 1;
				}
			}
		}
	}
	int result = 0;
	for (int i = 1; i <= n; i++)
		if (arr[1][i])
		{
			result++;
		}
	cout << result << endl;
	return 0;
}
