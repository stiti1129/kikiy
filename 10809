#include <iostream>
#include <string>

using namespace std;

int arr[26];

int main()
{
	string str;
	fill(arr, arr + 26,-1);
	getline(cin, str);
	for (int i = 0; i < str.length(); i++)
	{
		int temp = str.at(i) -'a';
		if (arr[temp] == -1)
			arr[temp] = i;
	}
	for (int i = 0; i < 26; i++)
		cout << arr[i] << ' ';
	cout << endl;
	return 0;
}
