#include <iostream>

using namespace std;

int arr[46];

bool go(int num)
{
	for (int i = 1; i <= num; i++)
	{
		for (int j = 1; j <= num; j++)
		{
			if (arr[i] + arr[j] > num)continue;
			for (int k = 1; k <= num; k++)
			{
				if (arr[i] + arr[j] + arr[k] == num)
					return true;
				if (arr[i] + arr[j] + arr[k] > num)
					break;
			}
		}
	}
	return false;
}
int main()
{
	int T;
	for (int i = 1; i <= 45; i++)
		arr[i] = (i*(i + 1)) >> 1;
	cin >> T;
	for (int test = 0; test < T; test++)
	{
		int num; cin >> num;
		cout << go(num) << endl;
	}
	return 0;
}
